{
  "name": "ada-toy-ui",
  "packageManager": "yarn@3.8.2",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*",
      "docs",
      "play"
    ]
  },
  "scripts": {
    "dev": "yarn workspace @adz-ui/play dev",
    "build": "yarn workspace build build",
    "docs:dev": "yarn workspace @adz-ui/docs dev",
    "docs:build": "yarn workspace @adz-ui/docs docs:build",
    "test": "yarn workspace @adz-ui/components test"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.12.13",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/tsconfig": "^0.5.1",
    "move-file-cli": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "postcss-color-mix": "^1.1.0",
    "postcss-each": "^1.1.0",
    "postcss-each-variables": "^0.3.0",
    "postcss-for": "^2.1.1",
    "postcss-nested": "^6.0.1",
    "release-it": "^17.3.0",
    "rimraf": "^5.0.7",
    "typescript": "^5.4.5",
    "vite": "^5.2.0",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.19"
  },
  "dependencies": {
    "@adz-ui/components": "workspace:^",
    "@adz-ui/constants": "workspace:^",
    "@adz-ui/hooks": "workspace:*",
    "@adz-ui/theme": "workspace:*",
    "@adz-ui/utils": "workspace:*",
    "adz-ui": "workspace:*",
    "vue": "^3.4.21"
  }
}
